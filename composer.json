{
  "name": "apitte/console",
  "description": "Symfony Console for Apitte stack",
  "keywords": [
    "apitte",
    "console",
    "symfony"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/console",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.3",
    "apitte/core": "~0.7 || ~0.8",
    "symfony/console": "^4.1.1|^5.0.0"
  },
  "require-dev": {
    "apitte/negotiation": "^0.7.0",
    "nette/di": "^3.0.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Console\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
